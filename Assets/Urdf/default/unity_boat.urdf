<?xml version="1.0" encoding="utf-8"?>
<robot name="boat">
  <material name="Green1">
    <color rgba="0 1 0 1" />
  </material>
  <material name="Cyan1">
    <color rgba="0 0.9 0.9 1" />
  </material>
  <material name="white">
    <color rgba="1 1 1 1" />
  </material>
  <material name="c1">
    <color rgba="0 0.6 0.8 1" />
  </material>
  <material name="c2">
    <color rgba="0.7 0.6 0.8 1" />
  </material>


  <link name="base_link">
    <visual>
      <geometry>
        <box size="1 0.8 0.6" />
      </geometry>
      <material name="Green1" />
    </visual>
    <collision>
      <geometry>
        <box size="1 0.8 0.6" />
      </geometry>
    </collision>
  </link>
  <link name="left_balancer">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.4 0.2 0.6" />
      </geometry>
      <material name="Cyan1" />
    </visual>
    <collision>
      <geometry>
        <box size="0.4 0.2 0.6" />
      </geometry>
    </collision>
  </link>
  <link name="right_balancer">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.4 0.2 0.6" />
      </geometry>
      <material name="Cyan1" />
    </visual>
    <collision>
      <geometry>
        <box size="0.4 0.2 0.6" />
      </geometry>
    </collision>
  </link>
  <link name="boat_rear">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.6 0.3 0.6" />
      </geometry>
      <material name="white" />
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.3 0.6" />
      </geometry>
    </collision>
  </link>
  <link name="engine">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.3 0.2 0.6" />
      </geometry>
      <material name="c1" />
    </visual>
    <collision>
      <geometry>
        <box size="0.3 0.2 0.6" />
      </geometry>
    </collision>
  </link>
  <link name="video_camera">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.05" />
      </geometry>
      <material name="c2" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.05" />
      </geometry>
    </collision>
  </link>


  <joint name="base_to_left_balancer" type="fixed">
    <origin xyz="0.7 0.3 0" />
    <parent link="base_link" />
    <child link="left_balancer" />
  </joint>

  <joint name="base_to_right_balancer" type="fixed">
    <origin xyz="0.7 -0.3 0" />
    <parent link="base_link" />
    <child link="right_balancer" />
  </joint>
  <joint name="base_to_boat_rear" type="fixed">
    <origin xyz="-0.8 0 0" />
    <parent link="base_link" />
    <child link="boat_rear" />
  </joint>
  <joint name="rear_to_engine" type="revolute">
    <origin xyz="-0.35 0 0" />
    <parent link="boat_rear" />
    <child link="engine" />
    <axis xyz="0 0 1" />
    <limit lower="-0.4" upper="0.4" effort="1000" velocity="1000" />
  </joint>
  <joint name="base_to_video_camera" type="fixed">
    <origin xyz="0.4 0 0.325" />
    <parent link="base_link" />
    <child link="video_camera" />
  </joint>
</robot>